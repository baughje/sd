source 'https://rubygems.org'

gem 'rails', '3.2.8'
gem 'pg'
gem 'therubyracer'

# Gems used only for assets and not required
# in production environments by default.
group :assets do
  gem 'sass-rails'
  gem 'coffee-rails'
  gem 'uglifier'
  gem 'bootstrap-sass'
end

group :development, :test do
  gem 'ffaker'
  gem 'rspec-rails'
  gem 'guard-rspec'
end

gem 'jquery-rails'
